#Author: Daniele Finadri 

.data
prompt: .asciz "Insert a string: "
output: .asciz "The string reversed is: "
str: .space 40
str_rev: .space 40

.text
.globl main
main:


li a7, 4
la a0, prompt
ecall

li a7, 8
li a1, 40
la a0, str
ecall

la t0, str
la t4, str_rev
jal reverse

la a0, output
li a7, 4
ecall
la a0, str_rev
li a7, 4
ecall 
li a7, 10
ecall

